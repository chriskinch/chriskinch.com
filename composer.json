{
    "name": "chriskinch/chriskinch8.com",
    "type": "project",
    "authors": [
        {
            "name": "chriskinch",
            "email": "kinchie@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": {
        "enquirejs": {
            "type": "package",
            "package": {
                "name": "wickynilliams/enquirejs",
                "version": "2.1.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/WickyNilliams/enquire.js/archive/v2.1.2.zip",
                    "type": "zip"
                }
            }
        },
        "css_inline": {
            "type": "package",
            "package": {
                "name": "chriskinch/css_inline",
                "version": "1.0",
                "type": "drupal-module",
                "dist": {
                    "url": "https://github.com/chriskinch/css_inline/archive/v1.0.zip",
                    "type": "zip"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "drupal/console": "1.0.0-beta5", 
        "wickynilliams/enquirejs": "2.1.2",
        "drupal/contact_block": "^1.3",
        "drupal/field_group": "1.0.0-rc6",
        "drupal/menu_link_attributes": "1.0.0-beta3",
        "drupal/breakpoints_settings": "1.x-dev",
        "chriskinch/css_inline": "1.0",
        "drupal/devel": "1.0.0-beta1"
    },
    "extra": {
        "installer-paths": {
            "../../core": ["type:drupal-core"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
